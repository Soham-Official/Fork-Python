https://practice.geeksforgeeks.org/problems/check-the-status/1/?track=python-module-2&batchId=119


def check_status(a, b, flag):
    if(flag==True and a<0 and b<0):
        return True
    if(a<0 and b>0 and flag==False):
        return True
    if(a>0 and b<0 and flag==False):
        return True
    else:
        return False
