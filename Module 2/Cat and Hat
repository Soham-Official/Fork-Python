def cat_hat(str):
    i=int(0)
    z=0
    x=0
    while(i<len(str)):
        if(str[i:i+3]=='cat'):
            x+=1;
        if(str[i:i+3]=='hat'):
            z+=1;
        i+=1
    # print(x,z)
    if(x is z):
        return True
    else:
        return False
