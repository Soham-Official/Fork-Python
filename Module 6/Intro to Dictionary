def create_dict(arr):
    
    dict = {}
    for i in  range(0,len(arr)):
        dict[arr[i][0]]=arr[i][1]
    # Your code here
    # Hint: use loop to iterate through arr
    # and assign key value to the dict
    
    
    
    return dict
