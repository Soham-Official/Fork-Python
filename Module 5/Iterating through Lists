def print_elements(N, arr):
    
    # Your code here 
    # use '//' to divide to get int for index
    x=N//2
    for i in range(0,x+1):
        print(arr[i],end=" ")
    for i in range(x+3,N,3):
        print(arr[i],end=" ")
